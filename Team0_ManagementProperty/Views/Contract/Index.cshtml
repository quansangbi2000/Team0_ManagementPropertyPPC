@model IEnumerable<Team0_ManagementProperty.Models.Full_Contract>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutAmin.cshtml";
}





<div class="right_col" role="main">
    <div class="">
        <div class="page-title">
            <div class="title_left">
                <a>Home - Contracts </a>
            </div>
        </div>
        <div class="clearfix"></div>
        <div class="row">
            <div class="col-md-12 col-sm-12 ">
                <div class="x_panel">
                    <div class="x_title">
                        <h2>Contracts Management</h2>
                        <div class="clearfix"></div>
                    </div>

                    <div class="x_content">
                        <div class="row">
                            <div class="col-sm-9">
                                <button type="button" class="btn btn-round btn-success">@Html.ActionLink("Create", "AddContract")</button>

                            </div>
                            <div class="col-sm-12">
                                <div class="card-box table-responsive">

                                    <table id="datatable" class="table table-striped table-bordered" style="width:100%">
                                        <thead>
                                            <tr>
                                                <th>
                                                    @Html.DisplayNameFor(model => model.Full_Contract_Code)
                                                </th>
                                                <th>
                                                    @Html.DisplayNameFor(model => model.Customer_Name)
                                                </th>
                                                <th>
                                                    @Html.DisplayNameFor(model => model.Year_Of_Birth)
                                                </th>
                                                <th>
                                                    @Html.DisplayNameFor(model => model.SSN)
                                                </th>
                                                <th>
                                                    @Html.DisplayNameFor(model => model.Customer_Address)
                                                </th>
                                                <th>
                                                    @Html.DisplayNameFor(model => model.Mobile)
                                                </th>
                                                <th>
                                                    @Html.DisplayNameFor(model => model.Date_Of_Contract)
                                                </th>
                                                <th>
                                                    @Html.DisplayNameFor(model => model.Price)
                                                </th>
                                                <th>
                                                    @Html.DisplayNameFor(model => model.Deposit)
                                                </th>
                                                <th>
                                                    @Html.DisplayNameFor(model => model.Remain)
                                                </th>
                                                <th>
                                                    @Html.DisplayNameFor(model => model.Status)
                                                </th>
                                                <th>
                                                    @Html.DisplayNameFor(model => model.Property.Property_Code)
                                                </th>
                                                <th></th>
                                            </tr>
                                        </thead>

                                        <tbody>
                                            @foreach (var item in Model)
                                            {
                                               <tr>
                                                   <td>
                                                       @Html.DisplayFor(modelItem => item.Full_Contract_Code)
                                                   </td>
                                                   <td>
                                                       @Html.DisplayFor(modelItem => item.Customer_Name)
                                                   </td>
                                                   <td>
                                                       @Html.DisplayFor(modelItem => item.Year_Of_Birth)
                                                   </td>
                                                   <td>
                                                       @Html.DisplayFor(modelItem => item.SSN)
                                                   </td>
                                                   <td>
                                                       @Html.DisplayFor(modelItem => item.Customer_Address)
                                                   </td>
                                                   <td>
                                                       @Html.DisplayFor(modelItem => item.Mobile)
                                                   </td>
                                                   <td>
                                                       @Html.DisplayFor(modelItem => item.Date_Of_Contract)
                                                   </td>
                                                   <td>
                                                       @Html.DisplayFor(modelItem => item.Price)
                                                   </td>
                                                   <td>
                                                       @Html.DisplayFor(modelItem => item.Deposit)
                                                   </td>
                                                   <td>
                                                       @Html.DisplayFor(modelItem => item.Remain)
                                                   </td>
                                                   <td>
                                                       @Html.DisplayFor(modelItem => item.Status)
                                                   </td>
                                                   <td>
                                                       @Html.DisplayFor(modelItem => item.Property.Property_Code)
                                                   </td>
                                                   <td>
                                                       @Html.ActionLink("Edit", "Edit", new { id = item.ID }) |
                                                       @Html.ActionLink("Details", "Details", new { id = item.ID }) |
                                                       @Html.ActionLink("Delete", "Delete", new { id = item.ID })
                                                   </td>
                                               </tr>

                                            }
                                        </tbody>
                                    </table>
                                </div>
                            </div>

                        </div>
                    </div>

                </div>

            </div>
        </div>
    </div>
</div>